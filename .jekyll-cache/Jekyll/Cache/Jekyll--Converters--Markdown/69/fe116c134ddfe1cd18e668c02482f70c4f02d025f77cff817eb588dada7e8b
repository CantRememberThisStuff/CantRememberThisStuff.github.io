    <ul id="bin-search-filters" class="filter-list">
	<h3>Attack Type:</h3>
	
	
	
	<li><input type="checkbox" id="Enumeration" name="attack_name" onclick="handleChange(this, &quot;Enumeration&quot;);" /><label id="Enumeration_label" for="Enumeration" data-title="Command focuses on enumeration.">Enumeration</label></li>
	
	
	
	<li><input type="checkbox" id="Exploitation" name="attack_name" onclick="handleChange(this, &quot;Exploitation&quot;);" /><label id="Exploitation_label" for="Exploitation" data-title="Command focuses on exploitation.">Exploitation</label></li>
	
	
	
	<li><input type="checkbox" id="Persistence" name="attack_name" onclick="handleChange(this, &quot;Persistence&quot;);" /><label id="Persistence_label" for="Persistence" data-title="Command focuses on persistence.">Persistence</label></li>
	
	
	
	<li><input type="checkbox" id="Privilege Escalation" name="attack_name" onclick="handleChange(this, &quot;Privilege Escalation&quot;);" /><label id="Privilege Escalation_label" for="Privilege Escalation" data-title="Command focuses on privilege escalation.">Privilege Escalation</label></li>
	
